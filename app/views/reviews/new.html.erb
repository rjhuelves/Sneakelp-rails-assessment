<%= form_for(@review) do |f| %>
    <div>
        <%= f.label :rating  %>
        <%= f.number_field :rating %>
    </div>
    <div>
        <%= f.label :title %>
        <%= f.text_field :title %>
    </div>
    <div>
        <%= f.label :content %>
        <%= f.text_field :content %>
    </div>
    <%= f.hidden_field :sneaker_id %>
    <br>
    <%= f.submit %>

<% end %>
